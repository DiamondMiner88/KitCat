{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "eofline": true,
        "indent": [true, "spaces", 4],
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-snake-case"]
    }
}
